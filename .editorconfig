# editorconfig.org

root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Indentation override for all JS under app directory
[app/**.js]
indent_style = space
indent_size = 2

# Matches .json
[*.json]
indent_style = space
indent_size = 2
